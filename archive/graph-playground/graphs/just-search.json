{
  "edges": [
    {
      "from": "secrets-1",
      "to": "urlTemplate-3",
      "constant": true,
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "secrets-1",
      "to": "urlTemplate-3",
      "constant": true,
      "out": "GOOGLE_CSE_ID",
      "in": "GOOGLE_CSE_ID"
    },
    {
      "from": "jsonata-5",
      "to": "output-6",
      "out": "result",
      "in": "text"
    },
    {
      "from": "fetch-4",
      "to": "jsonata-5",
      "out": "response",
      "in": "json"
    },
    {
      "from": "urlTemplate-3",
      "to": "fetch-4",
      "out": "url",
      "in": "url"
    },
    {
      "from": "input-2",
      "to": "urlTemplate-3",
      "out": "text",
      "in": "query"
    }
  ],
  "nodes": [
    {
      "id": "secrets-1",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PALM_KEY",
          "GOOGLE_CSE_ID"
        ]
      }
    },
    {
      "id": "input-2",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Query",
              "description": "What would you like to search for?"
            }
          },
          "required": [
            "text"
          ]
        }
      }
    },
    {
      "id": "urlTemplate-3",
      "type": "urlTemplate",
      "configuration": {
        "template": "https://www.googleapis.com/customsearch/v1?key={PALM_KEY}&cx={GOOGLE_CSE_ID}&q={query}"
      }
    },
    {
      "id": "fetch-4",
      "type": "fetch"
    },
    {
      "id": "jsonata-5",
      "type": "jsonata",
      "configuration": {
        "expression": "$join(items.snippet, '\n')"
      }
    },
    {
      "id": "output-6",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    }
  ]
}