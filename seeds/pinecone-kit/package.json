{
  "name": "@google-labs/pinecone-kit",
  "private": true,
  "version": "0.0.1",
  "description": "A collection of Breadboard nodes for working with Pinecone",
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "test": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard:build",
        "../core-kit:build",
        "../llm-starter:build",
        "build:tsc",
        "build:prepare"
      ]
    },
    "build:tsc": {
      "command": "tsc -b",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "../breadboard:build:tsc",
        "../core-kit:build:tsc",
        "../llm-starter:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "build:prepare": {
      "command": "node scripts/prepare.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "scripts/prepare.js"
      ],
      "output": [
        "docs/graphs/**/*.md",
        "graphs/**/*.json"
      ]
    },
    "test": {
      "command": "ava",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts"
      ],
      "output": []
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/labs-prototypes"
  },
  "files": [
    "dist/src",
    "graphs"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/google/labs-prototypes/issues"
  },
  "homepage": "https://github.com/google/labs-prototypes/tree/main/seeds/pinecone-kit#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@types/node": "^18.16.3",
    "ava": "^5.2.0",
    "typescript": "^5.0.4",
    "@google-labs/tsconfig": "*",
    "@google-labs/llm-starter": "*"
  },
  "dependencies": {
    "@google-labs/breadboard": "*",
    "@google-labs/core-kit": "*"
  }
}
