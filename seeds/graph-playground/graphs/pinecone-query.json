{
  "title": "Retrieval-augmented generation with Pinecone",
  "description": "This board implements the simples possible retrieval-augmented generation (RAG) system using Pinecone store. The store was generated with [pinecone-load](https://github.com/google/labs-prototypes/blob/main/seeds/graph-playground/graphs/pinecone-load.json).",
  "version": "0.0.1",
  "edges": [
    {
      "from": "secrets-3",
      "to": "embedString-2",
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "jsonata-5",
      "to": "promptTemplate-1",
      "out": "result",
      "in": "context"
    },
    {
      "from": "pinecone-query-4",
      "to": "jsonata-5",
      "out": "response",
      "in": "json"
    },
    {
      "from": "embedString-2",
      "to": "pinecone-query-4",
      "out": "embedding",
      "in": "embedding"
    },
    {
      "from": "query",
      "to": "embedString-2",
      "out": "text",
      "in": "text"
    },
    {
      "from": "query",
      "to": "promptTemplate-1",
      "out": "text",
      "in": "query"
    },
    {
      "from": "secrets-7",
      "to": "generateText-6",
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "generateText-6",
      "to": "rag",
      "out": "completion",
      "in": "text"
    },
    {
      "from": "promptTemplate-1",
      "to": "generateText-6",
      "out": "prompt",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "promptTemplate-1",
      "type": "promptTemplate",
      "configuration": {
        "template": "Analyze the question and the knowledge base, provided below.\n  \nIf the knowledge base does not contain the information to produce the answer, tell the user that you don't know.\n\nOtherwise, write a comprehensive answer to the question using only the information from the knowledge base.\n\n# Question:\n\n{{query}}\n\n# Knowledge Base:\n{{context}}\n\n# Answer\n"
      }
    },
    {
      "id": "query",
      "type": "input"
    },
    {
      "id": "embedString-2",
      "type": "embedString"
    },
    {
      "id": "secrets-3",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PALM_KEY"
        ]
      }
    },
    {
      "id": "pinecone-query-4",
      "type": "pinecone-query"
    },
    {
      "id": "jsonata-5",
      "type": "jsonata",
      "configuration": {
        "expression": "$join(matches.metadata.text, '\n\n')"
      }
    },
    {
      "id": "generateText-6",
      "type": "generateText"
    },
    {
      "id": "secrets-7",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PALM_KEY"
        ]
      }
    },
    {
      "id": "rag",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    },
    {
      "url": "npm:@google-labs/node-nursery"
    },
    {
      "url": "npm:@google-labs/pinecone-kit"
    }
  ]
}