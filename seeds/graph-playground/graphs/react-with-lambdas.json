{
  "edges": [
    {
      "from": "input-1",
      "to": "jsonata-3",
      "out": "tools",
      "in": "json"
    },
    {
      "from": "input-1",
      "to": "jsonata-4",
      "out": "tools",
      "in": "json"
    },
    {
      "from": "jsonata-4",
      "to": "promptTemplate-5",
      "constant": true,
      "out": "result",
      "in": "descriptions"
    },
    {
      "from": "jsonata-3",
      "to": "promptTemplate-5",
      "constant": true,
      "out": "result",
      "in": "tools"
    },
    {
      "from": "rememberObservation",
      "to": "promptTemplate-5",
      "out": "accumulator",
      "in": "memory"
    },
    {
      "from": "rememberThought",
      "to": "rememberObservation",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberObservation",
      "to": "rememberThought",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberQuestion",
      "to": "rememberThought",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberQuestion",
      "to": "promptTemplate-5",
      "out": "accumulator",
      "in": "memory"
    },
    {
      "from": "input-1",
      "to": "rememberQuestion",
      "out": "text",
      "in": "Question"
    },
    {
      "from": "secrets-2",
      "to": "react-completion",
      "constant": true,
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "react-completion",
      "to": "rememberThought",
      "out": "completion",
      "in": "Thought"
    },
    {
      "from": "react-completion",
      "to": "jsonata-6",
      "out": "completion",
      "in": "json"
    },
    {
      "from": "promptTemplate-5",
      "to": "react-completion",
      "out": "prompt",
      "in": "text"
    },
    {
      "from": "jsonata-6",
      "to": "jsonata-8",
      "out": "tool",
      "in": "tool"
    },
    {
      "from": "input-1",
      "to": "jsonata-8",
      "constant": true,
      "out": "tools",
      "in": "tools"
    },
    {
      "from": "jsonata-8",
      "to": "invoke-7",
      "out": "*"
    },
    {
      "from": "jsonata-6",
      "to": "invoke-7",
      "out": "args",
      "in": "text"
    },
    {
      "from": "invoke-7",
      "to": "rememberObservation",
      "out": "text",
      "in": "Observation"
    },
    {
      "from": "jsonata-6",
      "to": "output-9",
      "out": "answer",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "input-1",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Question",
              "description": "Ask ReAct a question"
            },
            "tools": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "tool": {
                    "type": "string",
                    "description": "The name of the tool."
                  },
                  "description": {
                    "type": "string",
                    "description": "A description of the tool."
                  },
                  "path": {
                    "type": "string",
                    "description": "The path to the tool.",
                    "format": "uri"
                  }
                },
                "required": [
                  "tool",
                  "description",
                  "path"
                ]
              }
            }
          },
          "required": [
            "text",
            "tools"
          ]
        }
      }
    },
    {
      "id": "secrets-2",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PALM_KEY",
          "GOOGLE_CSE_ID"
        ]
      }
    },
    {
      "id": "jsonata-3",
      "type": "jsonata",
      "configuration": {
        "expression": "*.tool ~> $join(', ')"
      }
    },
    {
      "id": "jsonata-4",
      "type": "jsonata",
      "configuration": {
        "expression": "$join(*.($$.tool & ': ' & $$.description), '\n')"
      }
    },
    {
      "id": "promptTemplate-5",
      "type": "promptTemplate",
      "configuration": {
        "template": "Answer the following questions as best you can. You have access to the following tools:\n\n{{descriptions}}\n\nUse the following format:\n\nQuestion: the input question you must answer\nThought: you should always think about what to do\nAction: the action to take, should be one of: {{tools}}\nAction Input: the input to the action\nObservation: the result of the action\n... (this Thought/Action/Action Input/Observation can repeat N times)\nThought: I now know the final answer\nFinal Answer: the final answer to the original input question\n\nBegin!\n\n{{memory}}\nThought:"
      }
    },
    {
      "id": "rememberObservation",
      "type": "append"
    },
    {
      "id": "rememberThought",
      "type": "append"
    },
    {
      "id": "rememberQuestion",
      "type": "append"
    },
    {
      "id": "react-completion",
      "type": "generateText",
      "configuration": {
        "stopSequences": [
          "\nObservation"
        ]
      }
    },
    {
      "id": "jsonata-6",
      "type": "jsonata",
      "configuration": {
        "expression": "{ 'tool': $match($, /Action: (.+)$/m).groups[0], 'args': $match($, /Action Input: (.+)$/m).groups[0], 'answer': $match($, /Final Answer: (.+)$/m).groups[0] }",
        "raw": true
      }
    },
    {
      "id": "invoke-7",
      "type": "invoke"
    },
    {
      "id": "jsonata-8",
      "type": "jsonata",
      "configuration": {
        "expression": "tools[tool = $$.tool]",
        "raw": true
      }
    },
    {
      "id": "output-9",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/core-kit"
    },
    {
      "title": "LLM Starter Kit",
      "description": "A kit that provides a few necessary components for wiring boards that use PaLM API.",
      "version": "0.0.1",
      "url": "npm:@google-labs/llm-starter"
    }
  ]
}