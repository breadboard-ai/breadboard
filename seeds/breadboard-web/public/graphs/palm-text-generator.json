{
  "title": "PaLM Text Generator",
  "description": "This text generator relies on the Google PaLM text-bison-001 model.",
  "version": "0.0.1",
  "edges": [
    {
      "from": "generateText",
      "to": "textOutput",
      "out": "completion",
      "in": "text"
    },
    {
      "from": "parameters",
      "to": "fn-3",
      "out": "useStreaming",
      "in": "useStreaming"
    },
    {
      "from": "parameters",
      "to": "generateText",
      "out": "text",
      "in": "text"
    },
    {
      "from": "secrets-4",
      "to": "generateText",
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    }
  ],
  "nodes": [
    {
      "id": "textOutput",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Text",
              "description": "The generated text"
            }
          }
        }
      }
    },
    {
      "id": "generateText",
      "type": "palm-generateText",
      "configuration": {}
    },
    {
      "id": "parameters",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Text",
              "description": "The text to generate"
            },
            "useStreaming": {
              "type": "boolean",
              "title": "Stream",
              "description": "Whether to stream the output",
              "default": "false"
            }
          },
          "required": [
            "text"
          ]
        }
      }
    },
    {
      "id": "secrets-4",
      "type": "secrets",
      "configuration": {
        "keys": [
          "PALM_KEY"
        ]
      }
    },
    {
      "id": "fn-3",
      "type": "invoke",
      "configuration": {
        "path": "#fn-3"
      }
    }
  ],
  "graphs": {
    "fn-3": {
      "edges": [
        {
          "from": "fn-3-input",
          "to": "fn-3-run",
          "out": "*"
        },
        {
          "from": "fn-3-run",
          "to": "fn-3-output",
          "out": "*"
        }
      ],
      "nodes": [
        {
          "id": "fn-3-input",
          "type": "input",
          "configuration": {}
        },
        {
          "id": "fn-3-run",
          "type": "runJavascript",
          "configuration": {
            "code": "function fn_3({useStreaming}) {if(useStreaming)throw new Error(\"Streaming is not supported by PaLM model\");return{}}",
            "name": "fn_3",
            "raw": true
          }
        },
        {
          "id": "fn-3-output",
          "type": "output",
          "configuration": {}
        }
      ]
    }
  }
}