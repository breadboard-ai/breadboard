{
  "name": "@google-labs/cloud-function",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "private": true,
  "version": "0.0.4",
  "description": "Experimenting with Breadboard and Cloud Functions",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "deploy": "wireit",
    "dev": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard-server:build"
      ]
    },
    "deploy": {
      "command": "./scripts/deploy.sh",
      "dependencies": [
        "build"
      ],
      "files": [
        "scripts/deploy.sh",
        "src/**/*.js"
      ],
      "output": []
    },
    "dev": {
      "command": "functions-framework --target=board",
      "service": true,
      "dependencies": [
        "build"
      ],
      "files": [
        "src/**/*.js"
      ]
    },
    "test": {
      "command": "ava",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build"
      ],
      "files": [
        "src/**/*.js",
        "tests/**/*.js"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/cloud-function",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.js"
    ],
    "workerThreads": false
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "ava": "^5.2.0"
  },
  "dependencies": {
    "@google-labs/breadboard-server": "^0.1.6",
    "@google-labs/breadboard": "^0.10.0",
    "@google-labs/template-kit": "^0.1.3",
    "dotenv": "^16.3.1"
  }
}
