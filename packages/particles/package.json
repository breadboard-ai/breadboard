{
  "name": "@breadboard-ai/particles",
  "version": "0.2.0",
  "description": "AI-first UI Toolkit",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./particles": "./dist/src/types.d.ts"
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:tsc": "wireit",
    "test": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    },
    "test": {
      "command": "node --test --enable-source-maps --test-reporter spec dist/tests/*.js",
      "dependencies": [
        "build"
      ],
      "files": [],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/particles",
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/particles#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1"
  }
}
