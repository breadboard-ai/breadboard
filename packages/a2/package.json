{
  "name": "@breadboard-ai/a2",
  "version": "0.6.0",
  "description": "Breadboard Components for building AI systems",
  "main": "./dist/src/index.js",
  "type": "module",
  "scripts": {
    "eval:agent": "tsx --experimental-test-module-mocks eval/agent.ts",
    "eval:a2ui": "tsx --experimental-test-module-mocks eval/a2ui.ts",
    "eval:autoname": "tsx --experimental-test-module-mocks eval/autoname.ts",
    "eval:viewer": "vite dev viewer",
    "build": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:tsc"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "../../core/tsconfig/base.json"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "lint": {
      "command": "eslint . --ext .ts --ignore-path ./.eslintignore",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        ".eslintignore",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/a2",
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git"
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.6",
    "@types/har-format": "^1.2.16",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.34.1",
    "dotenv": "^16.4.7",
    "idb": "^8.0.3",
    "inquirer": "^12.10.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@breadboard-ai/a2ui": "^0.1.0",
    "@breadboard-ai/data": "^0.1.0",
    "@breadboard-ai/embedded-board-server": "0.2.1",
    "@breadboard-ai/loader": "^0.0.3",
    "@breadboard-ai/mcp": "^0.1.0",
    "@breadboard-ai/types": "0.9.0",
    "@breadboard-ai/utils": "^0.1.0",
    "@cfworker/json-schema": "^4.1.1",
    "@google-labs/breadboard": "^0.36.0",
    "marked": "^16.3.0",
    "mime": "^4.1.0",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
  }
}
