{
  "name": "@breadboard-ai/a2",
  "version": "0.6.0",
  "description": "Breadboard Components for building AI systems",
  "main": "./dist/src/index.js",
  "type": "module",
  "scripts": {
    "push": "tsx scripts/push.ts",
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:tsc"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "../../core/tsconfig/base.json"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    }
  },
  "repository": {
    "directory": "packages/a2",
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git"
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@breadboard-ai/jsandbox": "^0.8.0",
    "@google-cloud/firestore": "^7.11.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.34.1",
    "dotenv": "^16.4.7",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@breadboard-ai/embedded-board-server": "0.2.1",
    "@breadboard-ai/loader": "^0.0.3",
    "@breadboard-ai/types": "0.9.0",
    "@breadboard-ai/utils": "^0.1.0",
    "@google-labs/breadboard": "^0.36.0",
    "marked": "^16.3.0"
  }
}
