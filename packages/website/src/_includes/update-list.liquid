<details open>
  <summary>Breadboard Updates</summary>
  <ul>
  {%- assign sorted_updates = collections.update | sort: 'date' | reverse -%}
  {%- for item in sorted_updates -%}
    <li>
      {% if page.url != item.url %}
        <a href="/breadboard{{ item.url }}">
      {% endif %}

        {{ item.data.title }}

      {% if page.url != item.url %}
        </a>
      {% endif %}
      </li>
  {%- endfor -%}
  </ul>
</details>
