{
  "title": "Hugging Face Question Answering Board",
  "description": "Board which calls the Hugging Face Question Answering Endpoint",
  "edges": [
    {
      "from": "fetch-5",
      "to": "main",
      "out": "*",
      "in": ""
    },
    {
      "from": "fn-3",
      "to": "fetch-5",
      "out": "auth",
      "in": "headers"
    },
    {
      "from": "fn-4",
      "to": "fetch-5",
      "out": "payload",
      "in": "body"
    },
    {
      "from": "main",
      "to": "output-2",
      "out": "output",
      "in": "output"
    },
    {
      "from": "query",
      "to": "fn-3",
      "out": "apiKey",
      "in": "key"
    },
    {
      "from": "query",
      "to": "fn-4",
      "out": "context",
      "in": "context"
    },
    {
      "from": "query",
      "to": "fn-4",
      "out": "question",
      "in": "question"
    }
  ],
  "nodes": [
    {
      "id": "query",
      "type": "input",
      "configuration": {
        "schema": {
          "title": "Hugging Face Schema For Question Answering",
          "properties": {
            "apiKey": {
              "type": "string",
              "title": "apiKey",
              "description": "The hugging face api key",
              "default": "myKey"
            },
            "context": {
              "type": "string",
              "title": "context",
              "description": "context for the question being asked",
              "default": "My name is Clara and I live in Berkeley."
            },
            "question": {
              "type": "string",
              "title": "question",
              "description": "The data to send to the hugging face api question answering endpoint",
              "default": "What is my name?"
            }
          }
        },
        "type": "string"
      }
    },
    {
      "id": "main",
      "type": "output",
      "configuration": {}
    },
    {
      "id": "output-2",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "output": {
              "type": "string",
              "title": "output"
            }
          }
        }
      }
    },
    {
      "id": "fetch-5",
      "type": "fetch",
      "configuration": {
        "method": "POST",
        "url": "https://api-inference.huggingface.co/models/deepset/roberta-base-squad2"
      }
    },
    {
      "id": "fn-3",
      "type": "runJavascript",
      "configuration": {
        "code": "const fn_3 = inputs=>{const key=inputs.key;const auth={Authorization:`Bearer ${key}`};return{auth}};",
        "name": "fn_3",
        "raw": true
      }
    },
    {
      "id": "fn-4",
      "type": "runJavascript",
      "configuration": {
        "code": "const fn_4 = input=>{const{question,context}=input;const payload={\"inputs\":{question,context}};return{payload}};",
        "name": "fn_4",
        "raw": true
      }
    }
  ]
}