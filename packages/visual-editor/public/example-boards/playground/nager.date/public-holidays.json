                  "fixed",
                  "global",
                  "counties",
                  "launchYear",
                  "types"
                ],
                "additionalProperties": false
              },
              "title": "Public Holidays",
              "description": "A list of public holidays for the selected country code and for the given year from the Nager Date API"
            }
          },
          "required": [
            "dates"
          ]
        }
      }
    },
    {
      "id": "fetchResult",
      "type": "fetch",
      "configuration": {
        "method": "GET"
      }
    },
    {
      "id": "spreadResponse",
      "type": "runJavascript",
      "configuration": {
        "code": "const spreadResponse = ({obj})=>{if(typeof obj!==\"object\"){throw new Error(`object is of type ${typeof obj} not object`)}return{results:{...obj}}};",
        "inputSchema": {
          "type": "object",
          "properties": {
            "obj": {
              "type": [
                "array",
                "boolean",
                "null",
                "number",
                "object",
                "string"
              ]
            }
          }
        },
        "name": "spreadResponse",
        "outputSchema": {
          "type": "object",
          "properties": {
            "results": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "date": {
                    "type": "string"
                  },
                  "localName": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "countryCode": {
                    "type": "string"
                  },
                  "fixed": {
                    "type": "boolean"
                  },
                  "global": {
                    "type": "boolean"
                  },
                  "counties": {
                    "anyOf": [
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "launchYear": {
                    "type": [
                      "number",
                      "null"
                    ]
                  },
                  "types": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": [
                  "date",
                  "localName",
                  "name",
                  "countryCode",
                  "fixed",
                  "global",
                  "counties",
                  "launchYear",
                  "types"
                ],
                "additionalProperties": false
              }
            }
          }
        },
        "raw": true
      },
      "metadata": {
        "title": "Spread",
        "description": "Spread the properties of the Nager Date response into a new object"
      }
    },
    {
      "id": "url",
      "type": "urlTemplate",
      "configuration": {
        "template": "https://date.nager.at/api/v3/PublicHolidays/{year}/{countryCode}"
      }
    }
  ]
