{
  "title": "Webcam",
  "description": "An example of using Gemini Kit's vision node with a webcam",
  "version": "0.0.2",
  "edges": [
    {
      "from": "combinePictureAndPrompt",
      "to": "describePicture",
      "out": "parts",
      "in": "parts"
    },
    {
      "from": "describePicture",
      "to": "output-2",
      "out": "result",
      "in": "text"
    },
    {
      "from": "input-1",
      "to": "combinePictureAndPrompt",
      "out": "picture",
      "in": "picture"
    },
    {
      "from": "input-1",
      "to": "combinePictureAndPrompt",
      "out": "prompt",
      "in": "prompt"
    }
  ],
  "nodes": [
    {
      "id": "input-1",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "picture": {
              "type": "object",
              "behavior": [
                "llm-content"
              ],
              "title": "Image",
              "format": "image-webcam"
            },
            "prompt": {
              "type": "string",
              "title": "Prompt",
              "examples": [
                "Describe what you see in the picture"
              ]
            }
          },
          "required": [
            "picture",
            "prompt"
          ]
        }
      }
    },
    {
      "id": "output-2",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Result"
            }
          }
        }
      }
    },
    {
      "id": "combinePictureAndPrompt",
      "type": "runJavascript",
      "configuration": {
        "code": "const combinePictureAndPrompt = ({picture,prompt})=>{const picturePart=picture.parts[0];return{parts:[picturePart,{text:prompt}]}};",
        "name": "combinePictureAndPrompt",
        "raw": true
      }
    },
    {
      "id": "describePicture",
      "type": "vision",
      "configuration": {}
    }
  ]
}