{
  "name": "@breadboard-ai/visual-editor",
  "version": "1.30.0",
  "description": "The Visual Editor for Breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:vite": "wireit",
    "test": "wireit",
    "eval:agent": "wireit",
    "eval:a2ui": "wireit",
    "eval:autoname": "wireit",
    "eval:viewer": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:vite"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "src/a2/**/*.bgl.json",
        "tests/**/*.ts",
        "tsconfig.json",
        "vite.config.ts",
        "../../core/tsconfig/base.json",
        "package.json"
      ]
    },
    "check:tsc": {
      "command": "tsc -b --pretty --noEmit",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": []
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [
        "dist/tsc/"
      ]
    },
    "build:vite": {
      "command": "vite build",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "check:tsc"
      ],
      "files": [
        "vite.config.ts",
        ".env*",
        "*.html",
        "oauth/",
        "experimental/",
        "public/",
        "landing/",
        "shell/",
        "icons/",
        "langs/"
      ],
      "output": [
        "dist/"
      ]
    },
    "prepare-assets": {
      "files": [
        "public/",
        "index.html",
        "icons/",
        "oauth",
        "landing/",
        "langs/",
        "shell"
      ]
    },
    "test": {
      "command": "node --test --enable-source-maps --test-reporter spec dist/tsc/tests/**/*.js dist/tsc/tests/*.js",
      "files": [],
      "output": [],
      "dependencies": [
        "build:tsc"
      ]
    },
    "eval:agent": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/agent.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:a2ui": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/a2ui/index.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:autoname": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/autoname.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:viewer": {
      "command": "vite dev eval/viewer",
      "files": [],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/visual-editor",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/",
    "public",
    "index.html"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/visual-editor#readme",
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@google-labs/tsconfig": "^0.0.2",
    "@types/flubber": "^0.4.0",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive-v3": "^0.0.5",
    "@types/google.picker": "^0.0.51",
    "@types/gtag.js": "^0.0.20",
    "@types/har-format": "^1.2.16",
    "@types/jsdom": "^27.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@breadboard-ai/types": "0.9.0",
    "@breadboard-ai/utils": "^0.1.0",
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.6",
    "@lit/task": "^1.0.2",
    "@material/material-color-utilities": "^0.3.0",
    "@material/web": "^2.3.0",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.calendar-v3": "^0.0.4",
    "@types/gapi.client.gmail-v1": "^0.0.4",
    "@types/google.picker": "^0.0.51",
    "@types/trusted-types": "^2.0.7",
    "ajv": "^8.17.1",
    "comlink": "^4.4.2",
    "flubber": "^0.4.2",
    "idb": "^8.0.3",
    "lit": "^3.3.1",
    "littlezipper": "^0.1.5",
    "markdown-it": "^14.1.0",
    "marked": "^16.3.0",
    "mimetext": "^3.0.27",
    "pdfjs-dist": "^5.4.449",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "urlpattern-polyfill": "^10.1.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  }
}
