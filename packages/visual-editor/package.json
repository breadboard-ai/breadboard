{
  "name": "@breadboard-ai/visual-editor",
  "version": "1.30.0",
  "description": "The Visual Editor for Breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:vite": "wireit",
    "build:tsc": "wireit",
    "test": "wireit",
    "eval:agent": "tsx --experimental-test-module-mocks eval/sets/agent.ts",
    "eval:a2ui": "tsx --experimental-test-module-mocks eval/sets/a2ui.ts",
    "eval:autoname": "tsx --experimental-test-module-mocks eval/sets/autoname.ts",
    "eval:viewer": "vite dev eval/viewer"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:vite",
        "build:tsc"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "vite.config.ts",
        "../../core/tsconfig/base.json",
        "package.json",
        "langs"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "build:vite": {
      "command": "vite build",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [
        "vite.config.ts",
        ".env*",
        "*.html",
        "oauth/",
        "experimental/",
        "public/"
      ],
      "output": [
        "dist/"
      ]
    },
    "prepare-assets": {
      "files": [
        "public/",
        "index.html",
        "icons/",
        "oauth",
        "landing/",
        "langs/",
        "shell"
      ]
    },
    "test": {
      "command": "node --test --enable-source-maps --test-reporter spec dist/tests/",
      "files": [],
      "output": [],
      "dependencies": [
        "build:tsc"
      ]
    }
  },
  "repository": {
    "directory": "packages/visual-editor",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/",
    "public",
    "index.html"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/visual-editor#readme",
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@google-labs/tsconfig": "^0.0.2",
    "@types/flubber": "^0.4.0",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive-v3": "^0.0.5",
    "@types/google.picker": "^0.0.51",
    "@types/gtag.js": "^0.0.20",
    "@types/har-format": "^1.2.16",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@breadboard-ai/a2ui": "^0.1.0",
    "@breadboard-ai/data": "^0.1.0",
    "@breadboard-ai/data-store": "0.5.0",
    "@breadboard-ai/google-drive-kit": "0.8.0",
    "@breadboard-ai/gulf": "0.1.0",
    "@breadboard-ai/mcp": "^0.1.0",
    "@breadboard-ai/particles": "0.2.0",
    "@breadboard-ai/particles-ui": "0.2.0",
    "@breadboard-ai/runtime": "0.2.0",
    "@breadboard-ai/theme": "0.0.3",
    "@breadboard-ai/types": "0.9.0",
    "@breadboard-ai/utils": "^0.1.0",
    "@google-labs/breadboard": "^0.36.0",
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.6",
    "@lit/task": "^1.0.2",
    "@material/web": "^2.3.0",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive-v3": "^0.0.5",
    "@types/google.picker": "^0.0.51",
    "@types/trusted-types": "^2.0.7",
    "ajv": "^8.17.1",
    "comlink": "^4.4.2",
    "flubber": "^0.4.2",
    "idb": "^8.0.3",
    "lit": "^3.3.1",
    "markdown-it": "^14.1.0",
    "pdfjs-dist": "^5.2.133",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "urlpattern-polyfill": "^10.1.0"
  }
}