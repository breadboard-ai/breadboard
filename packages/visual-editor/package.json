{
  "name": "@breadboard-ai/visual-editor",
  "version": "1.30.0",
  "description": "The Visual Editor for Breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "coverage": "wireit",
    "coverage:ci": "wireit",
    "build:vite": "wireit",
    "build:tsc": "wireit",
    "test": "wireit",
    "test:pattern": "wireit",
    "test:file": "wireit",
    "eval:agent": "wireit",
    "eval:a2ui": "wireit",
    "eval:autoname": "wireit",
    "eval:viewer": "wireit",
    "eval:viewer:build": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:vite"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "src/a2/**/*.bgl.json",
        "tests/**/*.ts",
        "tsconfig.json",
        "vite.config.ts",
        "../../core/tsconfig/base.json",
        "package.json"
      ]
    },
    "check:tsc": {
      "command": "tsc -b --pretty --noEmit",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": []
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [
        "dist/tsc/"
      ]
    },
    "build:vite": {
      "command": "vite build",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "check:tsc"
      ],
      "files": [
        "vite.config.ts",
        ".env*",
        "*.html",
        "oauth/",
        "experimental/",
        "public/",
        "landing/",
        "shell/",
        "icons/",
        "langs/"
      ],
      "output": [
        "dist/"
      ]
    },
    "prepare-assets": {
      "files": [
        "public/",
        "index.html",
        "icons/",
        "oauth",
        "landing/",
        "langs/",
        "shell"
      ]
    },
    "test": {
      "command": "node --import ./tests/controller-setup.js --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-reporter=lcov --test-reporter-destination=lcov.info --test --enable-source-maps './dist/tsc/tests/**/*.js' './dist/tsc/tests/*.js'",
      "env": {
        "NODE_V8_COVERAGE": "./coverage/tmp"
      },
      "files": [
        "tests/controller-setup.js"
      ],
      "output": [
        "lcov.info",
        "coverage/tmp",
        "test-storage"
      ],
      "dependencies": [
        "build:tsc"
      ]
    },
    "test:file": {
      "#": "Template for running a specific set of test files, usage: `npm run test:file --watch -- './dist/tsc/tests/sca/actions/share/**/*.js'`",
      "command": "node --import ./tests/controller-setup.js --test-reporter=spec --test --enable-source-maps",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "tests/controller-setup.js"
      ],
      "output": [
        "test-storage"
      ]
    },
    "test:pattern": {
      "#": "Run tests matching --test-name-pattern arg, usage: `npm run test:pattern -- --test-name-pattern=\"pattern\"`",
      "command": "node --import ./tests/controller-setup.js --test-reporter=spec --test --enable-source-maps './dist/tsc/tests/**/*.js' './dist/tsc/tests/*.js'",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "tests/controller-setup.js"
      ],
      "output": [
        "test-storage"
      ]
    },
    "coverage": {
      "command": "c8 --all report --reporter=html --reporter=json-summary --src=src",
      "files": [
        ".c8rc.json"
      ],
      "dependencies": [
        "test"
      ]
    },
    "coverage:ci": {
      "command": "mkdir -p coverage-ci && cp coverage/coverage-summary.json coverage-ci/",
      "files": [],
      "output": [
        "coverage-ci/coverage-summary.json"
      ],
      "dependencies": [
        "coverage"
      ]
    },
    "eval:agent": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/agent/index.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:a2ui": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/a2ui/index.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:autoname": {
      "command": "node --enable-source-maps --experimental-test-module-mocks dist/tsc/eval/sets/autoname.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "eval:viewer": {
      "command": "vite dev eval/viewer",
      "files": [],
      "output": []
    },
    "eval:viewer:build": {
      "command": "vite build eval/viewer --outDir ../../dist/eval-viewer",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "eval/viewer/**/*"
      ],
      "output": [
        "dist/eval-viewer"
      ]
    },
    "lint": {
      "command": "eslint 'src/**/*.ts'",
      "dependencies": [
        "<dependencies>#build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "eslint-rules/**/*.js",
        "../../eslint.config.js"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/visual-editor",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/",
    "public",
    "index.html"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/visual-editor#readme",
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@google-labs/tsconfig": "^0.0.2",
    "@tweakpane/core": "^2.0.5",
    "@types/flubber": "^0.4.0",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive-v3": "^0.0.5",
    "@types/google.picker": "^0.0.52",
    "@types/gtag.js": "^0.0.20",
    "@types/har-format": "^1.2.16",
    "@types/jsdom": "^27.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.0.0",
    "@types/node-localstorage": "^1.3.3",
    "c8": "^10.1.3",
    "dotenv": "^17.2.3",
    "fake-indexeddb": "^6.2.5",
    "google-auth-library": "^10.1.0",
    "jsdom": "^27.4.0",
    "node-localstorage": "^3.0.5",
    "prettier": "^3.8.1",
    "tsx": "^4.20.6",
    "tweakpane": "^4.0.5",
    "typescript": "^5.8.3",
    "vite": "^7.2.6",
    "vite-plugin-compression2": "^2.2.0",
    "wireit": "^0.15.0-pre.2"
  },
  "dependencies": {
    "@breadboard-ai/types": "0.9.0",
    "@breadboard-ai/utils": "^0.1.0",
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.6",
    "@lit/task": "^1.0.2",
    "@material/material-color-utilities": "^0.4.0",
    "@material/web": "^2.3.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.calendar-v3": "^0.0.4",
    "@types/gapi.client.gmail-v1": "^0.0.4",
    "@types/google.picker": "^0.0.52",
    "@types/trusted-types": "^2.0.7",
    "ajv": "^8.17.1",
    "comlink": "^4.4.2",
    "flubber": "^0.4.2",
    "idb": "^8.0.3",
    "lit": "^3.3.1",
    "littlezipper": "^0.1.5",
    "markdown-it": "^14.1.0",
    "marked": "^16.3.0",
    "mime": "^4.1.0",
    "mimetext": "^3.0.28",
    "pdfjs-dist": "^5.4.449",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "urlpattern-polyfill": "^10.1.0",
    "zod": "^4.3.6"
  }
}
