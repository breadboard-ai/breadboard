{
  "name": "@google-labs/create-breadboard",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "1.0.12",
  "description": "A starter kit for your Breadboard projects",
  "main": "dist/src/index.js",
  "bin": {
    "create-breadboard": "dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty && chmod +x dist/src/index.js",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/create-breadboard",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.1",
    "@types/node": "^20.14.8",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.13.1",
    "typescript": "^5.4.5"
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "@google-labs/breadboard-hello-world": "^1.2.14",
    "@google-labs/breadboard": "^0.20.0",
    "chalk": "^5.3.0"
  }
}
