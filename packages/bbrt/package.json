{
  "name": "@breadboard-ai/bbrt",
  "version": "0.0.1",
  "description": "BBRT",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git",
    "directory": "packages/bbrt"
  },
  "type": "module",
  "main": "index.js",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index.js": {
      "types": null,
      "default": null
    },
    "./internal/*.js": {
      "types": "./dist/internal/*.d.ts",
      "default": null
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!test/",
    "!src/",
    "!.wireit/"
  ],
  "scripts": {
    "build": "wireit",
    "build:tsc": "wireit",
    "test": "echo \"no tests yet\"",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc",
        "../breadboard:build",
        "../core-kit:build",
        "../template-kit:build"
      ]
    },
    "build:tsc": {
      "command": "tsc --pretty",
      "dependencies": [
        "../breadboard:build:tsc",
        "../core-kit:build:tsc",
        "../template-kit:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "tsconfig.base.json",
        "tsconfig.base.web.json"
      ],
      "output": [
        "dist/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "test:playwright": {
      "command": "web-test-runner \"dist/test/**/*.test.js\" --node-resolve --playwright --browsers chromium",
      "files": [],
      "output": [],
      "dependencies": [
        "build"
      ]
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.30.0",
    "@google-labs/core-kit": "^0.17.0",
    "@google-labs/template-kit": "^0.3.15",
    "@lit-labs/signals": "^0.1.1",
    "@types/json-schema": "^7.0.15",
    "jsonriver": "^1.0.0",
    "lit": "^3.2.1",
    "micromark": "^4.0.1",
    "micromark-extension-gfm": "^3.0.0",
    "signal-polyfill": "^0.2.1",
    "signal-utils": "^0.20.0"
  },
  "devDependencies": {
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-playwright": "^0.11.0",
    "typescript": "^5.6.3",
    "wireit": "^0.14.9"
  }
}
