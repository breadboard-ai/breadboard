{
  "name": "@breadboard-ai/bbrt",
  "version": "0.2.1",
  "description": "BBRT",
  "author": "Google LLC",
  "license": "Apache-2.0",
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git",
    "directory": "packages/bbrt"
  },
  "type": "module",
  "main": "index.js",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index.js": {
      "types": null,
      "default": null
    },
    "./internal/*.js": {
      "types": "./dist/internal/*.d.ts",
      "default": null
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!test/",
    "!src/",
    "!.wireit/"
  ],
  "scripts": {
    "build": "wireit",
    "build:tsc": "wireit",
    "test": "wireit",
    "test:node": "wireit",
    "test:playwright": "wireit",
    "lint": "wireit",
    "prepack": "npm run build"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc --pretty",
      "dependencies": [
        "<dependencies>#<this>",
        "../agent-kit#build",
        "../google-drive-kit#build"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "dependencies": [
        "test:node"
      ]
    },
    "test:node": {
      "command": "find dist/test/node -name '*_test.js' | xargs node --test --enable-source-maps --test-reporter spec",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "test:playwright": {
      "#comment": "TODO(aomarks) Actually run this; need to make sure GitHub actions are OK",
      "command": "web-test-runner \"dist/test/**/*_test.js\" --node-resolve --playwright --browsers chromium",
      "files": [],
      "output": [],
      "dependencies": [
        "build:tsc"
      ]
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@breadboard-ai/connection-client": "0.2.0",
    "@breadboard-ai/google-drive-kit": "0.5.1",
    "@breadboard-ai/shared-ui": "1.24.0",
    "@google-labs/agent-kit": "^0.17.0",
    "@google-labs/breadboard": "^0.33.0",
    "@google-labs/core-kit": "^0.18.1",
    "@google-labs/gemini-kit": "^0.11.2",
    "@google-labs/json-kit": "^0.3.16",
    "@google-labs/template-kit": "^0.3.18",
    "@lit-labs/signals": "^0.1.2",
    "@lit/context": "^1.1.4",
    "@types/json-schema": "^7.0.15",
    "jsonriver": "^1.0.0",
    "lit": "^3.2.1",
    "micromark": "^4.0.2",
    "micromark-extension-gfm": "^3.0.0",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@web/test-runner": "^0.20.0",
    "@web/test-runner-playwright": "^0.11.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.6",
    "wireit": "^0.15.0-pre.2"
  }
}
