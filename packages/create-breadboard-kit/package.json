{
  "name": "@google-labs/create-breadboard-kit",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "0.0.13",
  "description": "A starter kit for your breadboard kits",
  "main": "dist/src/index.js",
  "bin": {
    "create-breadboard-kit": "dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "copy-assets": "wireit",
    "build": "wireit",
    "build:tsc": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "copy-assets": {
      "command": "cp -r assets dist",
      "files": [
        "assets/"
      ],
      "output": [
        "dist/assets/"
      ]
    },
    "build": {
      "dependencies": [
        "build:tsc",
        "copy-assets"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/",
        "!dist/assets/"
      ],
      "clean": "if-file-deleted"
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/create-breadboard-kit",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/assets",
    "dist/src"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.1",
    "@types/node": "^20.14.8",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.13.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.20.0",
    "base-create": "^3.0.8"
  }
}
