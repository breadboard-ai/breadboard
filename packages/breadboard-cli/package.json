{
  "name": "@google-labs/breadboard-cli",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "0.7.1",
  "description": "A suite of tools for interacting with the Breadboard.",
  "bin": {
    "breadboard": "dist/src/index.js"
  },
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "test": "wireit",
    "build": "wireit",
    "build:tsc": "wireit",
    "build:debugger": "wireit",
    "lint": "wireit",
    "pack:install": "wireit",
    "test:pack": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard:build",
        "build:tsc",
        "build:debugger"
      ]
    },
    "build:debugger": {
      "command": "vite build",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "../breadboard-web:build:vite"
      ],
      "files": [
        "vite.config.ts",
        "src/debugger/index.html",
        "src/debugger/preview.html",
        "src/debugger/provider.ts"
      ],
      "output": [
        "dist/debugger"
      ]
    },
    "build:tsc": {
      "command": "tsc -b && chmod +x dist/src/index.js",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "../breadboard:build:tsc",
        "../breadboard-web:build:tsc",
        "../breadboard-web:generate:graphs",
        "../template-kit:build:tsc",
        "../core-kit:build:tsc",
        "../graph-integrity:build:tsc",
        "../node-nursery:build:tsc"
      ],
      "files": [
        "src/**/*.js",
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "ava",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "../../.eslintrc.json"
      ],
      "output": []
    },
    "pack:install": {
      "command": "npm pack && npm --verbose install -g $(find . -name \"$(npm pkg get name | tr -d '\"' | sed 's/@//;s/\\//-/g').tgz\")",
      "dependencies": [
        "build"
      ],
      "output": [
        "google-labs-breadboard-cli-*.tgz"
      ]
    },
    "test:pack": {
      "command": "(cd ~ && npx --verbose @google-labs/breadboard-cli --help)",
      "dependencies": [
        "pack:install"
      ]
    }
  },
  "repository": {
    "directory": "packages/breadboard-cli",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src",
    "dist/ui",
    "dist/debugger"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@google-labs/tsconfig": "^0.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.30",
    "@types/serve-handler": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.20.0",
    "ava": "^5.2.0",
    "openapi-types": "^12.1.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.13.0",
    "@google-labs/breadboard-web": "^1.4.0",
    "@google-labs/core-kit": "^0.5.1",
    "@google-labs/template-kit": "^0.2.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-virtual": "^3.0.2",
    "commander": "^12.0.0",
    "esbuild": "^0.20.1",
    "rollup": "^4.12.1",
    "serve": "^14.2.1",
    "urlpattern-polyfill": "^10.0.0",
    "vite": "^5.1.5",
    "yaml": "^2.4.1"
  }
}
