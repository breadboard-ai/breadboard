{
  "name": "@breadboard-ai/google-drive-kit",
  "version": "0.6.0",
  "description": "Nodes for reading & writing to files in Google Drive, including Docs and Sheets",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/google-drive-kit#readme",
  "repository": {
    "directory": "packages/google-drive-kit",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/google-drive-kit.git"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./index.js": {
      "types": null,
      "default": null
    },
    "./internal/*.js": {
      "types": "./dist/src/internal/*.d.ts",
      "default": null
    },
    "./*.js": {
      "types": "./dist/src/*.d.ts",
      "default": "./dist/src/*.js"
    },
    "./google-drive.kit.json": "./google-drive.kit.json"
  },
  "files": [
    "dist/**/*.{js,js.map,d.ts}",
    "!dist/test/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:tsc": "wireit",
    "test": "wireit",
    "coverage": "wireit",
    "lint": "wireit",
    "test-and-lint": "wireit",
    "dev": "npm run test-and-lint --watch"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "<dependencies>#<this>",
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc --pretty",
      "dependencies": [
        "<dependencies>#<this>"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "node --test --enable-source-maps --test-reporter spec dist/tests/**/*.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "tests/**/*.ts"
      ],
      "output": []
    },
    "coverage": {
      "command": "node --test --enable-source-maps --experimental-test-coverage --test-reporter lcov --test-reporter-destination=lcov.info dist/test/*_test.js",
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": [
        "lcov.info"
      ]
    },
    "lint": {
      "command": "eslint src/ --ext .ts",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    },
    "test-and-lint": {
      "dependencies": [
        "test",
        "lint"
      ]
    }
  },
  "dependencies": {
    "@breadboard-ai/connection-client": "0.2.0",
    "@breadboard-ai/types": "0.7.0",
    "@google-labs/breadboard": "^0.34.0",
    "@types/gapi.client.drive-v3": "^0.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^8.57.1",
    "typescript": "^5.8.3",
    "wireit": "^0.15.0-pre.2"
  }
}
