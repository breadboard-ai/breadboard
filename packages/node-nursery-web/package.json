{
  "name": "@google-labs/node-nursery-web",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "1.0.5",
  "description": "A Nursery for new nodes that run in both Web and Node environments. Move to other kits to publish.",
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "test": "wireit",
    "build": "wireit",
    "build:tsc": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard:build",
        "../template-kit:build",
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "../breadboard:build:tsc",
        "../core-kit:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "ava",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [],
      "output": []
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/node-nursery-web",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@google-labs/core-kit": "^0.5.1",
    "@google-labs/template-kit": "^0.2.2",
    "@google-labs/tsconfig": "^0.0.1",
    "@types/gapi": "^0.0.47",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.20.0",
    "ava": "^5.2.0",
    "npm": "^10.5.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.13.0",
    "firebase": "^10.8.1"
  }
}
