{
  "name": "@google-labs/breadboard-ui",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "0.7.1",
  "description": "The UI components for @google-labs/breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "generate:docs": "wireit",
    "dev": "wireit",
    "test": "wireit",
    "build": "wireit",
    "build:tsc": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "../breadboard:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "dev": {
      "command": "vite dev",
      "service": true,
      "dependencies": [
        {
          "script": "typescript-files-and-deps",
          "cascade": false
        }
      ],
      "files": [
        "index.html",
        "vite.config.ts"
      ]
    },
    "test": {
      "command": "vitest --run",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [
        "vite.config.ts"
      ],
      "output": []
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    },
    "generate:docs": {
      "command": "typedoc --plugin typedoc-plugin-markdown",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "docs/api/"
      ]
    }
  },
  "repository": {
    "directory": "packages/breadboard-ui",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src",
    "dist/*.js",
    "dist/*.js.map"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/breadboard#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "rollup": "^4.17.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^4.0.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.2",
    "@google-labs/breadboard": "^0.19.0",
    "@lit/task": "^1.0.0",
    "ajv": "^8.13.0",
    "lit": "^3.1.3",
    "markdown-it": "^14.1.0",
    "pixi.js": "^8.1.1"
  }
}
