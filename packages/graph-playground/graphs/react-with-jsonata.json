{
  "edges": [
    {
      "from": "reflect-4",
      "to": "jsonata-2",
      "out": "graph",
      "in": "json"
    },
    {
      "from": "reflect-4",
      "to": "jsonata-3",
      "out": "graph",
      "in": "json"
    },
    {
      "from": "jsonata-3",
      "to": "promptTemplate-5",
      "constant": true,
      "out": "result",
      "in": "descriptions"
    },
    {
      "from": "jsonata-2",
      "to": "promptTemplate-5",
      "constant": true,
      "out": "result",
      "in": "tools"
    },
    {
      "from": "rememberObservation",
      "to": "promptTemplate-5",
      "out": "accumulator",
      "in": "memory"
    },
    {
      "from": "rememberThought",
      "to": "rememberObservation",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberObservation",
      "to": "rememberThought",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberQuestion",
      "to": "rememberThought",
      "out": "accumulator",
      "in": "accumulator"
    },
    {
      "from": "rememberQuestion",
      "to": "promptTemplate-5",
      "out": "accumulator",
      "in": "memory"
    },
    {
      "from": "input-6",
      "to": "rememberQuestion",
      "out": "text",
      "in": "Question"
    },
    {
      "from": "secrets-1",
      "to": "react-completion",
      "constant": true,
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "math",
      "to": "rememberObservation",
      "out": "text",
      "in": "Observation"
    },
    {
      "from": "search",
      "to": "rememberObservation",
      "out": "text",
      "in": "Observation"
    },
    {
      "from": "jsonata-7",
      "to": "search",
      "out": "search",
      "in": "text"
    },
    {
      "from": "jsonata-7",
      "to": "math",
      "out": "math",
      "in": "text"
    },
    {
      "from": "jsonata-7",
      "to": "output-8",
      "out": "answer",
      "in": "text"
    },
    {
      "from": "react-completion",
      "to": "jsonata-7",
      "out": "completion",
      "in": "json"
    },
    {
      "from": "react-completion",
      "to": "rememberThought",
      "out": "completion",
      "in": "Thought"
    },
    {
      "from": "promptTemplate-5",
      "to": "react-completion",
      "out": "prompt",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "secrets-1",
      "type": "secrets",
      "configuration": {
        "keys": ["PALM_KEY", "GOOGLE_CSE_ID"]
      }
    },
    {
      "id": "jsonata-2",
      "type": "jsonata",
      "configuration": {
        "expression": "nodes.configuration.description.%.%.[id] ~> $join(', ')"
      }
    },
    {
      "id": "jsonata-3",
      "type": "jsonata",
      "configuration": {
        "expression": "nodes.configuration.description.%.%.[id &  ': ' & configuration.description] ~> $join('\n')"
      }
    },
    {
      "id": "reflect-4",
      "type": "reflect"
    },
    {
      "id": "promptTemplate-5",
      "type": "promptTemplate",
      "configuration": {
        "template": "Answer the following questions as best you can. You have access to the following tools:\n\n{{descriptions}}\n\nUse the following format:\n\nQuestion: the input question you must answer\nThought: you should always think about what to do\nAction: the action to take, should be one of: {{tools}}\nAction Input: the input to the action\nObservation: the result of the action\n... (this Thought/Action/Action Input/Observation can repeat N times)\nThought: I now know the final answer\nFinal Answer: the final answer to the original input question\n\nBegin!\n\n{{memory}}\nThought:"
      }
    },
    {
      "id": "rememberObservation",
      "type": "append"
    },
    {
      "id": "rememberThought",
      "type": "append"
    },
    {
      "id": "rememberQuestion",
      "type": "append"
    },
    {
      "id": "input-6",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Question",
              "description": "Ask ReAct a question"
            }
          },
          "required": ["text"]
        }
      }
    },
    {
      "id": "react-completion",
      "type": "palm-generateText",
      "configuration": {
        "stopSequences": ["\nObservation"]
      }
    },
    {
      "id": "math",
      "type": "include",
      "configuration": {
        "path": "https://raw.githubusercontent.com/breadboard-ai/breadboard/main/packages/graph-playground/graphs/math.json",
        "description": "Useful for when you need to solve math problems. Input should be a math problem to be solved."
      }
    },
    {
      "id": "search",
      "type": "include",
      "configuration": {
        "path": "https://raw.githubusercontent.com/breadboard-ai/breadboard/main/packages/graph-playground/graphs/search-summarize.json",
        "description": "Useful for when you need to find facts. Input should be a search query."
      }
    },
    {
      "id": "jsonata-7",
      "type": "jsonata",
      "configuration": {
        "expression": "($f := function($line, $str) { $contains($line, $str) ? $substring($line, $length($str)) }; $merge(($split('\n')[[1..2]]) @ $line.$.{'action': $f($line, 'Action: '), 'input': $f($line, 'Action Input: '),'answer': $f($line, 'Final Answer: ') }).{ action: input,'answer': answer})",
        "raw": true
      }
    },
    {
      "id": "output-8",
      "type": "output"
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/core-kit"
    },
    {
      "url": "npm:@google-labs/llm-starter"
    },
    {
      "url": "npm:@google-labs/palm-kit"
    }
  ]
}
