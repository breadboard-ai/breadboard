{
  "title": "The simplest LLM-based board",
  "description": "This is as simple as it gets: the board takes a prompt as input and generates a response as output.",
  "version": "0.0.2",
  "edges": [
    {
      "from": "secrets-2",
      "to": "palm-generateText-1",
      "out": "PALM_KEY",
      "in": "PALM_KEY"
    },
    {
      "from": "palm-generateText-1",
      "to": "completion",
      "out": "completion",
      "in": "text"
    },
    {
      "from": "prompt",
      "to": "palm-generateText-1",
      "out": "text",
      "in": "text"
    }
  ],
  "nodes": [
    {
      "id": "palm-generateText-1",
      "type": "palm-generateText"
    },
    {
      "id": "secrets-2",
      "type": "secrets",
      "configuration": {
        "keys": ["PALM_KEY"]
      }
    },
    {
      "id": "prompt",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Prompt",
              "description": "The prompt to generate a completion for",
              "examples": ["Tell me a fun story about playing with breadboards"]
            }
          },
          "required": ["text"]
        }
      }
    },
    {
      "id": "completion",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "text": {
              "type": "string",
              "title": "Response",
              "description": "The completion generated by the LLM"
            }
          },
          "required": ["text"]
        }
      }
    }
  ],
  "kits": [
    {
      "url": "npm:@google-labs/llm-starter"
    },
    {
      "url": "npm:@google-labs/palm-kit"
    }
  ]
}
