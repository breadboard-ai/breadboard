{
  "name": "@breadboard-ai/a2ui",
  "private": true,
  "version": "0.1.0",
  "description": "AI UI Web Library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/0.8/ui/ui.d.ts",
      "default": "./dist/0.8/ui/ui.js"
    }
  },
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:tsc": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [],
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "node --test --enable-source-maps --test-reporter spec dist/tests/*.js",
      "files": [],
      "output": [],
      "dependencies": [
        "build:tsc"
      ]
    }
  },
  "repository": {
    "directory": "packages/a2ui",
    "type": "git",
    "url": "git+https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/a2ui#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.4",
    "lit": "^3.3.1",
    "markdown-it": "^14.1.0",
    "signal-utils": "^0.21.1"
  }
}
