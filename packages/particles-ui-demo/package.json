{
  "name": "@breadboard-ai/particles-ui-demo",
  "private": true,
  "version": "0.0.1",
  "description": "AI-first UI Toolkit Demo",
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "wireit",
    "build:tsc": "wireit",
    "lint": "wireit",
    "dev": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "vite dev"
    },
    "build": {
      "dependencies": [
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b --pretty",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "<dependencies>#build"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/",
        "!dist/**/*.min.js{,.map}"
      ],
      "clean": "if-file-deleted"
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        ".eslintrc",
        "../../.eslintrc.json"
      ],
      "output": []
    }
  },
  "repository": {
    "directory": "packages/particles-ui-demo",
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/particles-ui-demo#readme",
  "devDependencies": {
    "@google-labs/tsconfig": "^0.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@breadboard-ai/particles": "0.0.1",
    "@breadboard-ai/particles-ui": "0.0.1",
    "@google/genai": "^1.6.0",
    "@lit-labs/signals": "^0.1.2",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@vavt/vite-plugin-import-markdown": "^1.0.1",
    "signal-utils": "^0.21.1"
  }
}
