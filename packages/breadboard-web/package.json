{
  "name": "@google-labs/breadboard-web",
  "private": true,
  "version": "0.0.1",
  "description": "The Web runtime for Breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/runtime.js",
    "./worker": "./dist/src/worker.js"
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "wireit",
    "deploy": "npm run build:vite && firebase deploy",
    "build": "wireit",
    "build:vite": "wireit",
    "test": "wireit",
    "generate:graphs": "wireit",
    "generate:docs": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard:build",
        "../breadboard-ui:build",
        "../core-kit:build",
        "../json-kit:build",
        "../llm-starter:build",
        "../node-nursery-web:build",
        "../palm-kit:build",
        "../pinecone-kit:build",
        "build:vite",
        "generate:graphs"
      ]
    },
    "typescript-files-and-deps": {
      "dependencies": [
        "../breadboard:build:tsc",
        "../breadboard-ui:build:tsc",
        "../core-kit:build:tsc",
        "../json-kit:build:tsc",
        "../llm-starter:build:tsc",
        "../node-nursery-web:build:tsc",
        "../palm-kit:build:tsc",
        "../pinecone-kit:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ]
    },
    "build:vite": {
      "command": "vite build",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps",
        "generate:graphs"
      ],
      "files": [
        "vite.config.ts",
        ".env",
        "index.html"
      ],
      "output": [
        "dist/"
      ]
    },
    "dev": {
      "command": "vite",
      "service": true,
      "dependencies": [
        {
          "script": "typescript-files-and-deps",
          "cascade": false
        },
        {
          "script": "generate:graphs",
          "cascade": false
        }
      ],
      "files": [
        "vite.config.ts"
      ]
    },
    "test": {
      "command": "vitest --run",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [
        "vite.config.ts"
      ],
      "output": []
    },
    "generate:graphs": {
      "command": "tsx src/make-graphs.ts",
      "dependencies": [
        "typescript-files-and-deps"
      ],
      "files": [],
      "output": [
        "public/local-boards.json",
        "public/graphs/**/*.json"
      ]
    },
    "generate:docs": {
      "command": "typedoc --plugin typedoc-plugin-markdown",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "docs/api/"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard"
  },
  "files": [
    "dist/src"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/breadboard-web#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@google-labs/tsconfig": "*",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "ava": "^5.2.0",
    "dotenv": "^16.3.1",
    "tsx": "^3.14.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.5",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-plugin-watch-and-run": "^1.4.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@google-labs/breadboard": "*",
    "@google-labs/breadboard-ui": "*",
    "@google-labs/core-kit": "*",
    "@google-labs/json-kit": "*",
    "@google-labs/llm-starter": "*",
    "@google-labs/node-nursery-web": "*",
    "@google-labs/palm-kit": "*",
    "@google-labs/pinecone-kit": "*",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.0"
  }
}
