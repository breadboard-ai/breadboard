          ]
        }
      }
    },
    {
      "id": "output",
      "type": "output",
      "configuration": {
        "schema": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "date": {
                "type": "string",
                "format": "date"
              },
              "localName": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "countryCode": {
                "type": "string"
              },
              "fixed": {
                "type": "boolean"
              },
              "global": {
                "type": "boolean"
              },
              "counties": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "launchYear": {
                "type": "integer"
              },
              "type": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    {
      "id": "fetch",
      "type": "fetch",
      "configuration": {
        "method": "GET"
      }
    },
    {
      "id": "urlTemplate",
      "type": "urlTemplate",
      "configuration": {
        "template": "https://date.nager.at/api/v3/PublicHolidays/{year}/{countryCode}"
      }
    }
  ]
