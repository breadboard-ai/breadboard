{
  "title": "Nager Date Public Holidays API",
  "description": "Get the public holidays for the Nager Date API",
  "version": "0.0.1",
  "edges": [
    {
      "from": "fetch",
      "to": "output",
      "out": "response",
      "in": "dates"
    },
    {
      "from": "query",
      "to": "urlTemplate",
      "out": "countryCode",
      "in": "countryCode"
    },
    {
      "from": "query",
      "to": "urlTemplate",
      "out": "year",
      "in": "year"
    },
    {
      "from": "urlTemplate",
      "to": "fetch",
      "out": "url",
      "in": "url"
    }
  ],
  "nodes": [
    {
      "id": "query",
      "type": "input",
      "configuration": {
        "schema": {
          "type": "object",
          "properties": {
            "year": {
              "title": "year",
              "type": "number",
              "description": "The data for year",
              "default": "2024"
            },
