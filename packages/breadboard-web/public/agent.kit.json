              "type": "runJavascript",
              "configuration": {
                "code": "function fn_4({title,description,context}) {const schema2={type:\"object\",properties:{text:{title,description,behavior:[\"transient\"]}}};return{schema:schema2,context}}",
                "name": "fn_4",
                "raw": true
