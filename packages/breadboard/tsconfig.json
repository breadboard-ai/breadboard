{
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "tests/**/*",
    "__tests__/**/*"
  ],
  "extends": "@google-labs/tsconfig/base.json",
  "typedocOptions": {
    "entryPoints": "./src/index.ts",
    "out": "./docs/api",
    "githubPages": "false"
  }
}
